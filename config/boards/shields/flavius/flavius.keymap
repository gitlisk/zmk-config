#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {  
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
            &kp KP_N5    &mo 1
            &kp KP_N5  &kp KP_N5
            >;
        }; 
        
        bt_layer {
            bindings = <
                &bootloader   &none
                &bt BT_SEL 0  &bt BT_CLR
            >;
        };
    };
};